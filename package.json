{
  "name": "@biswap/uikit",
  "version": "0.64.1",
  "description": "Set of UI components for Biswap projects",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/biswapcom/biswap-front-uikit",
  "license": "MIT",
  "scripts": {
    "start": "yarn storybook",
    "build": "rm -rf ./dist && rollup -c && tsc -d --emitDeclarationOnly --declarationDir dist",
    "watch": "rollup -c -w",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check --loglevel error 'src/**/*.{js,jsx,ts,tsx}'",
    "format:write": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test": "jest",
    "prepublishOnly": "yarn build"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "8.3.4",
    "@rollup/plugin-url": "7.0.0",
    "@storybook/addon-a11y": "6.5.10",
    "@storybook/addon-actions": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/addon-links": "6.5.10",
    "@storybook/react": "6.5.10",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@types/lodash": "4.14.182",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "4.4.5",
    "@types/styled-components": "5.1.26",
    "i": "0.3.7",
    "jest-styled-components": "7.1.0",
    "npm": "8.17.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "rollup": "2.78.0",
    "styled-components": "5.3.5",
    "themeprovider-storybook": "^1.8.0",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "5.3.5"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@storybook/addon-postcss": "^2.0.0",
    "@types/styled-system": "5.1.15",
    "autoprefixer": "^10.4.8",
    "classnames": "2.3.2",
    "lodash": "4.17.21",
    "postcss-flexbugs-fixes": "^5.0.2",
    "react-popper": "2.3.0",
    "react-transition-group": "4.4.5",
    "storybook-addon-styled-component-theme": "^2.0.0",
    "styled-system": "5.1.5",
    "react-paginate": "8.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
